<div class="alignleft">
  <h2><a href="<%- config.root %>"><%= config.title %></a></h2>
  <h3><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></h3>
</div>
<div class="alignright" style="margin-top: 15px">
<% if(theme.twitter.username){ %>
  <a href="https://twitter.com/<%- theme.twitter.username %>"><div class="outlink twitter"></div></a>
<% } %>
<% if(theme.github.username){ %>
  <a href="https://github.com/<%- theme.github.username %>"><div class="outlink github"></div></a>
<% } %>
</div>
<nav id="main-nav">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>

<div class="clearfix"></div>
